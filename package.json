{
  "name": "brixo-ifsc",
  "version": "1.0.0",
  "private": true,
  "description": "IFSC details service with Express, TypeScript, MongoDB (Mongoose), Redis cache, and Razorpay provider",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "ifsc",
    "express",
    "typescript",
    "mongoose",
    "redis",
    "razorpay"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.4",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "mongoose": "^8.5.1",
    "morgan": "^1.10.0",
    "pino": "^9.3.2",
    "pino-http": "^10.3.0",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.14.9",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "^9.10.0",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
